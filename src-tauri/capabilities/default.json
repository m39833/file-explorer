{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:allow-default-urls",
    {
      "identifier": "opener:allow-open-path",
      "allow": [{ "path": "**" }]
    },
    "opener:allow-open-url",
    "opener:allow-reveal-item-in-dir"
  ]
}

